{
  "eslint.validate": [
    "typescript"
  ],
  "eslint.alwaysShowStatus": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "gitlab.instanceUrl": "https://git.oxfordnanolabs.local",
  "stencilTools.component.alphabetizeImports": true,
  "stencilTools.component.defaultImports": [
    "Component",
    "Host",
    "h",
    "Prop"
  ],
  "stencilTools.quotes": "single",
  "stencilTools.style.extension": "scss",
  "stencilTools.test.extension": "spec.ts",
  "tslint.ignoreDefinitionFiles": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "json.schemas": [
    {
      "fileMatch": [
        "/examples/reports/*.json",
        "/www/examples/reports/*.json",
        "/**/*.cronkite.json"
      ],
      "url": "/src/cronkite.schema.json"
    }
  ]
}
